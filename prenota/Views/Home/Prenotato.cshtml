@model List<Utente>;
@{
    ViewData["Title"] = "Home Page";
}
 <h2>Grazie @Model[0].Nome</h2>
 <p>per esserti registrato</p>
 <p>Sono stati registrati @Model.Count() utenti</p>

 <table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Nome</th>
            <th>Cognome</th>
            <th>Tipo</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @{foreach(var item in @Model){
        <tr>
            <td>@item.Nome</td>
            <td>@item.Cognome</td>
            <td>@item.Tipo</td>
            <td>@item.Email</td>
        </tr>
        }
    }
    </tbody>
 </table>
 